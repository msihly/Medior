{
  "compilerOptions": {
    "module": "CommonJS",
    "noEmit": false,
    "outDir": "extraResources",
    "paths": {
      "medior/*": ["./medior/*"]
    },
    "types": ["node", "vite/client", "vite-plugin-svgr/client"]
  },
  "extends": "./tsconfig.base.json",
  "exclude": ["./ext", "./generator", "./logs", "./node_modules", "./MongoDB", "./patches"],
  "include": [
    "./medior/_generated",
    "./medior/main.ts",
    "./medior/server.ts",
    "./medior/database",
    "./medior/face-models"
  ]
}
